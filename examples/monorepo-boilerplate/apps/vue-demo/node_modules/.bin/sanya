#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jv/Desktop/project/sanya/packages/sanya/bin/node_modules:/Users/jv/Desktop/project/sanya/packages/sanya/node_modules:/Users/jv/Desktop/project/sanya/packages/node_modules:/Users/jv/Desktop/project/sanya/node_modules:/Users/jv/Desktop/project/node_modules:/Users/jv/Desktop/node_modules:/Users/jv/node_modules:/Users/node_modules:/node_modules:/Users/jv/Desktop/project/sanya/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jv/Desktop/project/sanya/packages/sanya/bin/node_modules:/Users/jv/Desktop/project/sanya/packages/sanya/node_modules:/Users/jv/Desktop/project/sanya/packages/node_modules:/Users/jv/Desktop/project/sanya/node_modules:/Users/jv/Desktop/project/node_modules:/Users/jv/Desktop/node_modules:/Users/jv/node_modules:/Users/node_modules:/node_modules:/Users/jv/Desktop/project/sanya/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sanya/bin/sanya.js" "$@"
else
  exec node  "$basedir/../sanya/bin/sanya.js" "$@"
fi
